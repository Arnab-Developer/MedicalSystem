@using MedicalSystem.FrontEnds.WebMvc.Models
@model IEnumerable<DoctorModel>
@{
    ViewData["Title"] = "Doctor";
}

<div class="text-center">
    <h1 class="display-4">Doctor</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>First name</th>
                <th>Last name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var doctor in Model)
            {
                <tr>
                    <td>@doctor.Id</td>
                    <td>@doctor.FirstName</td>
                    <td>@doctor.LastName</td>
                </tr>
            }
        </tbody>
    </table>
</div>