@using MedicalSystem.FrontEnds.WebMvc.Models
@model IEnumerable<DoctorModel>
@{
    ViewData["Title"] = "Doctor";
}

<div class="text-center">
    <h1 class="display-4">Doctor</h1>
    <table>
        <tr>
            <td>Id</td>
            <td>First name</td>
            <td>Last name</td>
        </tr>
        @foreach (var doctor in Model)
        {
            <tr>
                <td>@doctor.Id</td>
                <td>@doctor.FirstName</td>
                <td>@doctor.LastName</td>
            </tr>
        }
    </table>
</div>